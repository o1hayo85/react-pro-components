{
  "packages": [
    "packages/*"
  ],
  "npmClient": "yarn",
  "version": "0.4.43",
  "command": {
    "publish": {
      "ignoreChanges": [
        "ignored-file",
        "*.md"
      ],
      "message": "chore(release): publish",
      "registry": "https://registry.npmjs.org/"
    }
  },
  "ignoreChanges": [
    "**/*.md",
    "**/__tests__/**"
  ]
}
